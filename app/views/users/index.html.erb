<div class="user_nav"><span class="test"><%= link_to 'Home', posts_path %></span></div>
<h2>Listing Users</h2>

<table class="home">
  <tr>
    <th>Username</th>
    <th>Email</th>
	<th>Access Token</th>
    <th>Actions</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.username %></td>
    <td><%= user.email %></td>
	<td><%= user.access_token %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %> | <%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<span class="test"><%= link_to 'New User', new_user_path %></span>
