<div class="user_nav"><span class="sbutton"><%= link_to 'Home', posts_path %></span></div>
<h2>Listing Users</h2>

<table class="home">
  <tr>
    <th>Username</th>
    <th>Email</th>
	<th>Score</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.username %></td>
    <td><%= user.email %></td>
	<td><%= user.reputation_for(:votes).to_i %></td>
  </tr>
<% end %>
</table>

<br />

<span class="sbutton"><%= link_to "Sign up with Google", "/auth/google_oauth2" %></span>
<br /><br />
